    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "web-vitals": "^3.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^18.0.0",
    "axios": "^1.3.0",
    "chart.js": "^4.2.0",
    "react-chartjs-2": "^5.2.0",
    "recharts": "^2.5.0",
